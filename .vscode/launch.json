    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) turretrc webserver Launch",
            "type": "lldb",
            "request": "launch",
            "program": "/usr/bin/node",
            "preLaunchTask": "npm: build:dev",
            "args" : "${workspaceFolder}/../turretRC/ChipServer/webserver.js",
            "env": {
                "DEBUG": "false",
                "PORT": "8080"
            }
        }
    ]
}